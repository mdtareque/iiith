### Assignment 3 : RPC webservice creation

build-n-run.sh
  
  #!/bin/bash
  
  #shopt -s globstar
  #javac **/*.java
  javac -cp ".:../lib/commons-net-3.0.1.jar" mtk/ias/bittorrent/v2/MiniBitTorrent.java
  jar cvfe mini-bittorrent-tareque-a1.jar mtk.ias.bittorrent.v2.MiniBitTorrent mtk/*
  #jar tvf mini-bittorrent-tareque-a1.jar
  java -cp lib/commons-net-3.0.1.jar:mini-bittorrent-tareque-a1.jar mtk.ias.bittorrent.v2.MiniBitTorrent
  

